<templateSet group="Xaml">
  <template name="strres" value="&lt;data name=&quot;$KEY$&quot; xml:space=&quot;preserve&quot;&gt;&#10;    &lt;value&gt;$VALUE$&lt;/value&gt;&#10;&lt;/data&gt;" description="generate string resource entry" toReformat="false" toShortenFQNames="true">
    <variable name="KEY" expression="" defaultValue="ResourceKey" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="ResourceValue" alwaysStopAt="true" />
    <context>
      <option name="XML" value="true" />
    </context>
  </template>
  <template name="bindres" value="{Binding $KEY$, Source={StaticResource Resources}}" description="Bind string resource to XAML" toReformat="false" toShortenFQNames="true">
    <variable name="KEY" expression="" defaultValue="resource key" alwaysStopAt="true" />
    <context>
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="HTML" value="false" />
      <option name="Handlebars" value="false" />
      <option name="JSON" value="false" />
      <option name="OTHER" value="true" />
      <option name="XML" value="false" />
    </context>
  </template>
</templateSet>